{
  "name": "nextjs-mantine-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "dev": "next dev",
    "build": "next build",
    "git:commit": "cz",
    "git:push": "git push -u origin HEAD",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
    "find:unused": "next-unused",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@hookform/resolvers": "^3.3.1",
    "@mantine/core": "^6.0.21",
    "@mantine/dates": "^6.0.21",
    "@mantine/dropzone": "^6.0.21",
    "@mantine/form": "^6.0.21",
    "@mantine/hooks": "^6.0.21",
    "@mantine/next": "^6.0.21",
    "@mantine/notifications": "^6.0.21",
    "@next/font": "^13.2.4",
    "@sentry/browser": "^7.42.0",
    "@sentry/nextjs": "^7.42.0",
    "@svgr/webpack": "^8.0.1",
    "@tabler/icons-react": "2.17.0",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-table": "^8.7.9",
    "@unpic/react": "^0.0.17",
    "axios": "^0.27.2",
    "commitlint": "^17.4.4",
    "dayjs": "^1.11.7",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "image-config": "^1.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mantine-contextmenu": "^6.0.1",
    "mantine-datatable": "^2.9.14",
    "mantine-react-table": "^1.3.1",
    "mapbox-gl": "^2.14.1",
    "maplibre-gl": "^2.4.0",
    "next": "^13.2.4",
    "next-auth": "^4.22.1",
    "next-real-viewport": "^0.7.2",
    "nextjs-google-analytics": "^2.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-map-gl": "^7.0.23",
    "react-query": "^3.39.3",
    "unpic": "^3.0.1",
    "xstate": "^4.37.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.8",
    "zustand-middleware-xstate": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify/react": "^4.1.1",
    "@next/bundle-analyzer": "^13.2.4",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-table": "^7.7.14",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "8.12.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "next-sitemap": "^3.1.55",
    "prettier": "^2.8.4",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.js"
    }
  }
}
